{
  "name": "earnings-google-calendar",
  "version": "0.0.3",
  "description": "Fetch scheduled company earnings, dividends, CPI report dates and other info into google calendar.",
  "main": "app/app.ts",
  "type": "module",
  "scripts": {
    "run": "npx tsx app/app.ts",
    "sync-cpi-events": "npx tsx ./commands/sync-cpi-events.command.ts",
    "sync-earnings-events": "npx tsx ./commands/sync-earnings-events.command.ts",
    "sync-market-holiday-events": "node commands/sync-market-holiday-events.command.ts",
    "sync-dividend-events": "npx tsx ./commands/sync-dividend-events.command.ts",
    "sync-fed-events": "npx tsx ./commands/sync-fed-events.command.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junajan/earnings-google-calendar.git"
  },
  "keywords": [
    "trading",
    "stock",
    "earnings",
    "dividends",
    "cpi reports",
    "market holidays",
    "fed events",
    "google calendar"
  ],
  "author": {
    "name": "Jan Juna",
    "email": "mail@janjuna.cz",
    "url": "https://janjuna.cz/"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/junajan/earnings-google-calendar/issues"
  },
  "homepage": "https://github.com/junajan/earnings-google-calendar#readme",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "cheerio": "1.0.0-rc.12",
    "config": "^3.3.11",
    "googleapis": "^137.1.0",
    "nyse-holidays": "^1.2.0",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "yahoo-finance2": "^2.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.13.0"
  }
}
